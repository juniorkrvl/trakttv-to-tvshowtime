{
  "name": "TraktTVShow",
  "description": "Syncs Trakt.tv history with TVShowTime app",
  "region": "us-west-2",
  "runtime": "python2.7",
  "handler": "main.handler",
  "role": "arn:aws:iam::602300322755:role/lambda-execution-role",
  "requirements": ["requests"],
  "ignore": [
    "circle\\.yml$",
    "\\.git$",
    "\\.pyc$",
    "\\.vscode/.*$",
    "\\.zip$",
    "pip",
    "venv"
  ],
  "timeout": 60,
  "memory": 512
}